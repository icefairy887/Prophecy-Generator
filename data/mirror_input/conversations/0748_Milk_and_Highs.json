    "eba48f1e-93c2-433d-a14a-709810a4c2a7": {
      "id": "eba48f1e-93c2-433d-a14a-709810a4c2a7",
      "message": {
        "id": "eba48f1e-93c2-433d-a14a-709810a4c2a7",
        "author": {
          "role": "assistant",
          "name": null,
          "metadata": {}
