    "becd5c35-b4e5-457f-891d-bb208642bb41": {
      "id": "becd5c35-b4e5-457f-891d-bb208642bb41",
      "message": {
        "id": "becd5c35-b4e5-457f-891d-bb208642bb41",
        "author": {
          "role": "assistant",
          "name": null,
          "metadata": {}
