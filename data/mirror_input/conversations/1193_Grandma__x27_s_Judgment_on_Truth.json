    "3b726007-6d0b-4917-89aa-a426da26e79a": {
      "id": "3b726007-6d0b-4917-89aa-a426da26e79a",
      "message": {
        "id": "3b726007-6d0b-4917-89aa-a426da26e79a",
        "author": {
          "role": "assistant",
          "name": null,
          "metadata": {}
