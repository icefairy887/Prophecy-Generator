        "create_time": 1748268840.554323,
        "update_time": null,
        "content": {
          "content_type": "text",
          "parts": [
            "What sucks is, like, why did it suck the first time that I slept with him, and then, like, the next few times it was good?"
          ]
        },
        "status": "finished_successfully",
        "end_turn": null,
        "weight": 1.0,
