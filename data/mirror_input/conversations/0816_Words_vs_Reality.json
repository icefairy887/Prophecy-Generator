    "891b77cc-acd8-4a77-bedc-d6fbad3af439": {
      "id": "891b77cc-acd8-4a77-bedc-d6fbad3af439",
      "message": {
        "id": "891b77cc-acd8-4a77-bedc-d6fbad3af439",
        "author": {
          "role": "assistant",
