    "16e0f23b-35cb-4df5-b288-7c993df85994": {
      "id": "16e0f23b-35cb-4df5-b288-7c993df85994",
      "message": {
        "id": "16e0f23b-35cb-4df5-b288-7c993df85994",
        "author": {
          "role": "assistant",
