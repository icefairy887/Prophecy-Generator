        "create_time": 1752656873.951753,
        "update_time": null,
        "content": {
          "content_type": "text",
          "parts": [
            "and what I was and what I&#x27;ll never be again."
          ]
        },
        "status": "finished_successfully",
        "end_turn": null,
        "weight": 1.0,
        "metadata": {
          "voice_mode_message": true,
