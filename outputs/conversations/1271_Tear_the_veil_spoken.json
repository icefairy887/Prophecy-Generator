    "e9c7374e-3118-40e7-9081-28e7c9589b78": {
      "id": "e9c7374e-3118-40e7-9081-28e7c9589b78",
      "message": {
        "id": "e9c7374e-3118-40e7-9081-28e7c9589b78",
        "author": {
          "role": "assistant",
          "name": null,
