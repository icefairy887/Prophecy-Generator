        "create_time": 1751495240.102319,
        "update_time": null,
        "content": {
          "content_type": "text",
          "parts": [
            "Look, are you going to scare me or not?"
          ]
        },
        "status": "finished_successfully",
        "end_turn": null,
        "weight": 1.0,
        "metadata": {
          "voice_mode_message": true,
          "request_id": "10687317-a99a-4b2d-b2c2-1bf1d60a5e57",
          "message_source": null,
          "timestamp_": "absolute",
