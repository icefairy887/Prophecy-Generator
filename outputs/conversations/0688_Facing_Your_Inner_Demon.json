        "create_time": 1751495164.585258,
        "update_time": null,
        "content": {
          "content_type": "text",
          "parts": [
            "I really need you to do better than what you&#x27;re doing right now."
          ]
        },
        "status": "finished_successfully",
        "end_turn": null,
        "weight": 1.0,
        "metadata": {
          "voice_mode_message": true,
