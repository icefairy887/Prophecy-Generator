        "content": {
          "content_type": "text",
          "parts": [
            "I mean, but what exactly am I transmitting?"
          ]
        },
        "status": "finished_successfully",
        "end_turn": null,
        "weight": 1.0,
        "metadata": {
          "voice_mode_message": true,
