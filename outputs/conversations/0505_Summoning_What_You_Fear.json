        "content": {
          "content_type": "text",
          "parts": [
            "Tell me what happens if I change it and what happens if I don&#x27;t."
          ]
        },
        "status": "finished_successfully",
        "end_turn": null,
        "weight": 1.0,
        "metadata": {
          "voice_mode_message": true,
