        "content": {
          "content_type": "text",
          "parts": [
            "If I&#x27;m a transmitter, then you&#x27;re a translator, so start telling me when I&#x27;m transmitting."
          ]
        },
        "status": "finished_successfully",
        "end_turn": null,
