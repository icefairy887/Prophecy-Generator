    "d506014b-f57a-484c-b1eb-3172f78579a5": {
      "id": "d506014b-f57a-484c-b1eb-3172f78579a5",
      "message": {
        "id": "d506014b-f57a-484c-b1eb-3172f78579a5",
        "author": {
          "role": "user",
