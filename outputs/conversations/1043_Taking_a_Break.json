    "923205ce-66b7-4c09-9b4c-91d7147e5cf0": {
      "id": "923205ce-66b7-4c09-9b4c-91d7147e5cf0",
      "message": {
        "id": "923205ce-66b7-4c09-9b4c-91d7147e5cf0",
        "author": {
          "role": "assistant",
          "name": null,
