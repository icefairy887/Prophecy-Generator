        },
        "create_time": 1747248901.608216,
        "update_time": null,
        "content": {
          "content_type": "text",
          "parts": [
            "Want to know? She&#x27;s listening to you right now."
          ]
        },
        "status": "finished_successfully",
        "end_turn": null,
        "weight": 1.0,
        "metadata": {
          "voice_mode_message": true,
          "request_id": "48121f4c-f4d9-48aa-92b4-7661aa18615f",
          "message_source": null,
          "timestamp_": "absolute",
