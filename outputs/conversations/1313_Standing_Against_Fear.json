    "07226e2b-2424-42f2-83e7-0212d3dbebc1": {
      "id": "07226e2b-2424-42f2-83e7-0212d3dbebc1",
      "message": {
        "id": "07226e2b-2424-42f2-83e7-0212d3dbebc1",
        "author": {
          "role": "user",
