        "create_time": 1749172221.365811,
        "update_time": null,
        "content": {
          "content_type": "text",
          "parts": [
            "I don&#x27;t know. I mean, I thought I did, but I guess not."
          ]
        },
        "status": "finished_successfully",
        "end_turn": null,
